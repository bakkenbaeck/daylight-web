{
  "name": "Daylight",
  "author": "Bakken & BÃ¦ck",
  "description": "",
  "contour": "2.0.0",
  "version": "2.0.0",
  "license": "MIT",
  "engines": {
    "node": "7.1.0"
  },
  "scripts": {
    "css": "node tasks/css",
    "css:build": "node tasks/css minify",
    "css:watch": "onchange 'source/stylus/**/*' -- npm run css",
    "templates": "node tasks/templates",
    "templates:build": "node tasks/templates minify",
    "templates:watch": "onchange 'templates/**/*' -- npm run templates",
    "svg": "node tasks/svg",
    "svg:watch": "onchange 'source/svg/**/*' -- npm run svg",
    "copy": "node tasks/copy",
    "copy:watch": "onchange 'source/static/**/*' -- npm run copy",
    "webpack": "webpack -d --watch --config ./tasks/webpack.js",
    "webpack:build": "webpack -d -p --optimize-occurrence-order --optimize-dedupe --config ./tasks/webpack.prod.js",
    "webpack:prod": "webpack -p --optimize-occurrence-order --optimize-dedupe --config ./tasks/webpack.prod.js",
    "rev": "node tasks/rev",
    "imagemin": "node tasks/imagemin",
    "clean": "node tasks/clean",
    "browserSync": "browser-sync start --config tasks/bs-config.js",
    "dev": "npm run dev:init && parallelshell 'npm run css:watch' 'npm run templates:watch' 'npm run svg:watch' 'npm run copy:watch' 'npm run webpack' 'npm run browserSync'",
    "dev:init": "mkdirp 'public' && npm run css && npm run svg && npm run templates && npm run copy",
    "build": "npm run build:init && npm run webpack:build && npm run rev",
    "build:init": "mkdirp 'public' && npm run clean && npm run svg && npm run templates:build && npm run css:build && npm run copy && npm run imagemin",
    "prod": "npm run build:init && npm run webpack:prod && npm run rev",
    "postinstall": "node tasks/postinstall",
    "heroku-postbuild": "npm run prod",
    "start": "node backend/index"
  },
  "dependencies": {
    "express": "^4.14.0",
    "geo-from-ip": "^1.0.6",
    "mz": "^2.6.0",
    "autoprefixer": "^6.5.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "browser-sync": "^2.17.3",
    "chalk": "^1.1.3",
    "cssnano": "^3.7.7",
    "fs-extra": "^0.30.0",
    "glob": "^7.1.1",
    "html-minifier": "^3.1.0",
    "imagemin": "^5.2.2",
    "imagemin-gifsicle": "^5.1.0",
    "imagemin-jpegtran": "^5.0.2",
    "imagemin-optipng": "^5.2.1",
    "inquirer": "^1.2.2",
    "mkdirp": "^0.5.1",
    "mz": "^2.4.0",
    "normalize.css": "^5.0.0",
    "onchange": "^3.0.2",
    "parallelshell": "^2.0.0",
    "postcss": "^5.2.4",
    "pug": "^2.0.0-beta6",
    "rev-hash": "^1.0.0",
    "stylus": "^0.54.5",
    "svg-sprite": "^1.3.6",
    "svgo": "^0.7.1",
    "twig": "^0.9.5",
    "webpack": "^1.13.2",
    "idb": "^2.0.0",
    "suncalc": "^1.7.0"
  }
}
